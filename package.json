{
  "name": "mqtt-savior",
  "version": "1.0.0",
  "description": "mqtt 救星",
  "main": "./dist/main/index.js",
  "scripts": {
    "start": "tsc && webpack && electron .",
    "debug": "MODE=debug npm start",
    "check": "eslint src/  --ext .ts,.tsx --ignore-path .gitignore && tsc",
    "pack": "electron-packager . mq-savior --overwrite --platform=linux --arch=x64 --asar --out package --ignore configs",
